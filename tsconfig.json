{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2021"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src"
  }
}
